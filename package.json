{
  "name": "mochaccino",
  "version": "1.2.0-alpha",
  "description": "United js test tools",
  "keywords": [
    "mocha",
    "test",
    "bdd",
    "tdd"
  ],
  "main": "lib/index.js",
  "scripts": {
    "pretest": "babel src -d lib",
    "test": "npm run lint && mocha --compilers js:babel-register",
    "prepublish": "npm test",
    "lint": "eslint src test",
    "clean": "rm -rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawelgalazka/mochaccino.git"
  },
  "author": "Pawel Galazka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pawelgalazka/mochaccino/issues"
  },
  "homepage": "https://github.com/pawelgalazka/mochaccino#readme",
  "dependencies": {
    "chai": "3.4.0",
    "jsdom": "9.2.1",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0"
  },
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "6.9.0",
    "babel-cli": "6.10.1",
    "eslint": "1.9.0",
    "mocha": "2.3.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
